{
  "name": "aframe-jade-docker-boilerplate",
  "description": "Boilerplate for creating WebVR scenes using A-Frame and Jade templates, with Dockerfile for easy deployability.",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "help": "pug --help",
    "start": "npm run build && node server.js",
    "build": "pug -o build *.pug",
    "prod": "npm run build && pm2 start server.js --no-daemon -i 0",
    "dev": "npm run build && budo --dir build/ --live --verbose --port 3000 --open",
    "ghpages": "ghpages"
  },
  "devDependencies": {
    "budo": "^7.0.0",
    "ghpages": "0.0.3",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6"
  },
  "keywords": [
    "aframe",
    "aframe-example",
    "aframe-scene",
    "aframe-boilerplate",
    "aframe-jade-boilerplate",
    "aframe-docker"
  ],
  "dependencies": {
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "morgan": "^1.7.0"
  }
}
